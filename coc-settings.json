  {
    "diagnostic.enable": true,
    "coc.preferences.colorSupport": true,
    "coc.preferences.extensionUpdateCheck": "weekly",
    "highlight.disableLanguages": [
      "markdown"
    ],
    "codeLens.enable": false,
    "diagnostic.virtualText": false,
    "codeLens.separator": "<>",
    "suggest.minTriggerInputLength": 4,
    "suggest.floatEnable": true,
    "suggest.maxCompleteItemCount": 10,
    "suggest.removeDuplicateItems": true,
    "suggest.enablePreview": true,
    "suggest.noselect": true,
    "suggest.completionItemKindLabels": {
      "keyword": "\uf1de",
      "variable": "\ue79b",
      "value": "\uf89f",
      "operator": "\u03a8",
      "constructor": "\uf0ad",
      "function": "\u0192",
      "reference": "\ufa46",
      "constant": "\uf8fe",
      "method": "\uf09a",
      "struct": "\ufb44",
      "class": "\uf0e8",
      "interface": "\uf417",
      "text": "\ue612",
      "enum": "\uf435",
      "enumMember": "\uf02b",
      "module": "\uf40d",
      "color": "\ue22b",
      "property": "\ue624",
      "field": "\uf9be",
      "unit": "\uf475",
      "event": "\ufacd",
      "file": "\uf723",
      "folder": "\uf114",
      "snippet": "\ue60b",
      "typeParameter": "\uf728",
      "default": "\uf29c"
    },
    "explorer.previewAction.onHover": false,
    "explorer.icon.enableNerdfont": true,
    "explorer.keyMappings.global": {
      "<c-t>": "open:tab",
      "<c-v>": "open:vsplit",
      "<c-h>": "open:split"
    },
    "explorer.file.hiddenRules": {
      "extensions": [
        "o",
        "a",
        "obj",
        "pyc"
      ],
      "filenames": [],
      "patternMatches": [
        "(^\\.|index.md|img)"
      ]
    },
    "highlight.colorNames.enable": false,
    "snippets.ultisnips.enable": true,
    "snippets.loadFromExtensions": false,
    "snippets.extends": {
      "cpp": [
        "c"
      ],
      "html": [
        "javascript"
      ],
      "zsh": [
        "sh"
      ]
    },
    "pairs.disableLanguages": [
      "racket",
      "scheme"
    ],
    "pairs.enableCharacters": [
      "(",
      "[",
      "{",
      "'",
      "\"",
      "`"
    ],
    "git.enableGutters": false,
    "cmake.lsp.enable": true,
    "go.goplsOptions": {
      "usePlaceholders": true
    },
    "languageserver": {
      "ccls": {
        "command": "ccls",
        "filetypes": [
          "c",
          "cpp",
          "objc",
          "objcpp"
        ],
        "rootPatterns": [
          ".ccls",
          "compile_commands.json",
          ".vim/",
          ".git/",
          ".hg/"
        ],
        "initializationOptions": {
          "cache": {
            "directory": "/tmp/ccls"
          }
        }
      }
    }
  }
