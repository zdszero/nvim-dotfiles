{
  "diagnostic.enable": true,
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.source.file.priority": 10,
  "codeLens.enable": false,
  "codeLens.separator": "<>",
  "diagnostic.virtualText": false,
  "inlayHint.enable": true,
  "outline.autoPreview": false,
  "outline.expandLevel": 2,
  "tree.key.toggle": "o",
  "tree.key.collapseAll": "M",
  "imselect.defaultInput": "com.apple.keylayout.ABC",
  "semanticTokens.enable": false,
  "signature.enable": true,
  "signature.target": "float",
  "signature.preferShownAbove": false,
  "suggest.minTriggerInputLength": 2,
  "suggest.maxCompleteItemCount": 10,
  "suggest.removeDuplicateItems": true,
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "suggest.snippetsSupport": true,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "constructor": "\uf0ad",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "explorer.previewAction.onHover": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings.global": {
    "<c-t>": "open:tab",
    "<c-v>": "open:vsplit",
    "<c-h>": "open:split"
  },
  "explorer.file.hiddenRules": {
    "extensions": [
      "o",
      "a",
      "obj",
      "pyc"
    ],
    "filenames": [
      "__pycache__"
    ],
    "patternMatches": [
      "(^\\.)",
      ".*egg-info"
    ]
  },
  "snippets.priority": 100,
  "snippets.ultisnips.pythonPrompt": false,
  "snippets.ultisnips.directories": [
    "UltiSnips"
  ],
  "snippets.ultisnips.enable": true,
  "snippets.snipmate.enable": false,
  "snippets.loadFromExtensions": false,
  "snippets.extends": {
    "latex": [
      "tex",
      "texmath"
    ],
    "cuda": [
      "cpp"
    ]
  },
  "snippets.userSnippetsDirectory": "~/.config/nvim/UltiSnips",
  "pairs.disableLanguages": [
    "racket",
    "scheme"
  ],
  "pairs.enableCharacters": [
    "(",
    "[",
    "{",
    "'",
    "\"",
    "`"
  ],
  "cmake.lsp.enable": false,
  "pyright.completion.snippetSupport": false,
  "pyright.disableProgressNotifications": true,
  "pyright.testing.provider": "pytest",
  "python.pythonPath": "/usr/bin/python3",
  "python.formatting.provider": "black",
  "clangd.disableSnippetCompletion": false,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "usePlaceholders": true,
        "analyses": {
          "fillstruct": true
        }
      }
    }
  }
}
