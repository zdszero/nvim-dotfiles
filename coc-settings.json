{
  "diagnostic.enable": true,
  "coc.preferences.extensionUpdateCheck": "weekly",
  "highlight.disableLanguages": [
    "markdown"
  ],
  "codeLens.enable": false,
  "diagnostic.virtualText": false,
  "codeLens.separator": "<>",
  "signature.target": "float",
  "suggest.minTriggerInputLength": 2,
  "suggest.floatEnable": true,
  "suggest.maxCompleteItemCount": 10,
  "suggest.removeDuplicateItems": true,
  "suggest.enablePreview": true,
  "suggest.noselect": true,
  "suggest.snippetsSupport": true,
  "explorer.previewAction.onHover": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings.global": {
    "<c-t>": "open:tab",
    "<c-v>": "open:vsplit",
    "<c-h>": "open:split"
  },
  "explorer.file.hiddenRules": {
    "extensions": [
      "o",
      "a",
      "obj",
      "pyc"
    ],
    "filenames": [
      "__pycache__",
      "img",
      "image"
    ],
    "patternMatches": [
      "(^\\.)",
      ".*egg-info"
    ]
  },
  "highlight.colorNames.enable": false,
  "snippets.ultisnips.directories": [
    "UltiSnips"
  ],
  "snippets.ultisnips.enable": true,
  "snippets.snipmate.enable": false,
  "snippets.loadFromExtensions": false,
  "snippets.extends": {
    "latex": ["tex", "texmath"]
  },
  "pairs.disableLanguages": [
    "racket",
    "scheme"
  ],
  "pairs.enableCharacters": [
    "(",
    "[",
    "{",
    "'",
    "\"",
    "`"
  ],
  "cmake.lsp.enable": true,
  "Lua.diagnostics.globals": ["vim"],
  "pyright.completion.snippetSupport": false,
  "python.pythonPath": "/usr/bin/python3.10",
  "python.formatting.provider": "autopep8",
  "python.formatting.autopep8Path": "/home/zds/.local/bin/autopep8",
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/13.0.0/clangd_13.0.0/bin/clangd",
  "clangd.disableSnippetCompletion": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  }
}
